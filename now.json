{
	"version": 2,
	"name":" pspk",
	"routes": [{ "src":"/(.*)", "dest":"/handler$1" }],
	"builds": [{ "src": "handler/*.go", "use": "@now/go", "config": { "maxLambdaSize": "10mb" }}],
	"build": {
		"env": {
			"GO111MODULE": "on"
		}
	},
	"env":{
		"DB_USER": "@mongo-name",
		"DB_PASS": "@mongo-pass",
		"DB_HOSTS": "@mongo-hosts"
	},
	"alias": ["pspk.now.sh"]
}
